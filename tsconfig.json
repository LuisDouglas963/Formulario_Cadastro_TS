{
  "compilerOptions": {
    "target": "es2019",
    "module": "CommonJS",
    "lib": ["DOM", "ESNext"],
    "outDir": "src/dist/assets/js",
    "rootDir": "./src",
    "removeComments": true,
    "noEmitOnError": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["./src"]
}
